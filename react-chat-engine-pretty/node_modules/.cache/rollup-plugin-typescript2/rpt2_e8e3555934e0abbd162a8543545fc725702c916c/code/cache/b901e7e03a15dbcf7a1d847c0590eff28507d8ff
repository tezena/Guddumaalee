{"code":"import React, { useRef, useEffect, useState } from 'react';\r\nimport axios from 'axios';\r\nimport { PlusOutlined } from '@ant-design/icons';\r\nimport Select from 'react-select';\r\nconst ChatForm = (props) => {\r\n    const didMountRef = useRef(false);\r\n    const [options, setOptions] = useState([]);\r\n    const [selected, setSelected] = useState([]);\r\n    useEffect(() => {\r\n        if (!didMountRef.current) {\r\n            didMountRef.current = true;\r\n            getAllUsers(users => {\r\n                const otherUsers = users.filter(user => user.username !== props.username);\r\n                const options = otherUsers.map(user => {\r\n                    return {\r\n                        value: JSON.stringify(user),\r\n                        label: `${user.first_name} ${user.last_name}`,\r\n                    };\r\n                });\r\n                setOptions(options);\r\n            }, () => console.log('oops!'));\r\n        }\r\n    });\r\n    const getAllUsers = (onSuccess, onError) => {\r\n        axios\r\n            .get('https://api.chatengine.io/users/search/', {\r\n            headers: {\r\n                'Project-ID': props.projectId,\r\n                'User-Name': props.username,\r\n                'User-Secret': props.secret,\r\n            },\r\n        })\r\n            .then(r => onSuccess(r.data))\r\n            .catch(() => onError);\r\n    };\r\n    return (React.createElement(\"div\", { className: \"ce-custom-chat-form\", style: styles.chatForm },\r\n        React.createElement(Select, { options: options, autoFocus: true, isMulti: true, onChange: users => {\r\n                setSelected(users);\r\n                props.onChange(users);\r\n            }, styles: customStyles, placeholder: \"Search for users...\" }),\r\n        React.createElement(\"button\", { className: \"ce-create-chat-button\", style: {\r\n                ...styles.createChatButton,\r\n                ...(selected.length === 0 && styles.createChatButtonDisabled),\r\n            }, onClick: props.onCancel, disabled: selected.length === 0 },\r\n            React.createElement(PlusOutlined, null)),\r\n        React.createElement(\"style\", null, `.ce-create-chat-button:hover { background-color: #7b3bc8; }`)));\r\n};\r\nconst customStyles = {\r\n    container: () => ({\r\n        width: 'calc(100% - 12px - 12px - 38px - 4px)',\r\n        marginLeft: '12px',\r\n        marginTop: '26px',\r\n    }),\r\n    control: (provided) => ({\r\n        ...provided,\r\n        outline: 'none',\r\n        fontFamily: 'Avenir',\r\n        color: 'white',\r\n        backgroundColor: 'white',\r\n        borderRadius: '8px',\r\n        border: '1px solid rgb(123, 59, 153)',\r\n        boxShadow: 'rgb(123, 59, 180 / 35%) 0px 2px 7px',\r\n    }),\r\n    input: (provided) => ({\r\n        ...provided,\r\n        color: 'white',\r\n    }),\r\n    placeholder: (provided) => ({\r\n        ...provided,\r\n        color: 'rgb(197, 197, 197)',\r\n    }),\r\n    option: (provided, state) => ({\r\n        ...provided,\r\n        fontFamily: 'Avenir',\r\n        color: 'rgb(197, 197, 197)',\r\n        backgroundColor: state.isFocused ? 'rgb(242, 242, 242)' : 'white',\r\n    }),\r\n    noOptionsMessage: (provided) => ({\r\n        ...provided,\r\n        fontFamily: 'Avenir',\r\n    }),\r\n    menu: (provided) => ({\r\n        ...provided,\r\n        width: 'calc(100% - 12px - 12px - 38px - 4px)',\r\n        backgroundColor: 'white',\r\n    }),\r\n};\r\nconst styles = {\r\n    chatForm: {\r\n        display: 'inline-block',\r\n        position: 'relative',\r\n        width: '100%',\r\n        height: '86px',\r\n    },\r\n    createChatButton: {\r\n        width: '38px',\r\n        height: '38px',\r\n        position: 'absolute',\r\n        right: '12px',\r\n        top: '26px',\r\n        borderRadius: '8px',\r\n        cursor: 'pointer',\r\n        transition: 'all 0.44s ease',\r\n        outline: 'none',\r\n        backgroundColor: 'rgb(123, 59, 153)',\r\n        border: '1px solid rgb(123, 59, 153)',\r\n        fontSize: '18px',\r\n        color: 'white',\r\n        boxShadow: 'rgba(123, 59, 153, 0.35) 0px 5px 15px',\r\n    },\r\n    createChatButtonDisabled: {\r\n        cursor: 'not-allowed',\r\n        color: 'white',\r\n        backgroundColor: '#7b3bc8',\r\n        border: '1px solid #7b3bc8',\r\n        boxShadow: 'none',\r\n    },\r\n};\r\nexport default ChatForm;\r\n//# sourceMappingURL=ChatForm.js.map","references":["B:/projects/2024 projects/Gudummaale/Gudduumalee/react-chat-engine-pretty/node_modules/@types/react/index.d.ts","B:/projects/2024 projects/Gudummaale/Gudduumalee/react-chat-engine-pretty/node_modules/axios/index.d.ts","B:/projects/2024 projects/Gudummaale/Gudduumalee/react-chat-engine-pretty/node_modules/react-chat-engine-advanced/dist/index.d.ts","B:/projects/2024 projects/Gudummaale/Gudduumalee/react-chat-engine-pretty/node_modules/@ant-design/icons/lib/index.d.ts","B:/projects/2024 projects/Gudummaale/Gudduumalee/react-chat-engine-pretty/node_modules/react-select/dist/react-select.cjs.d.ts"],"map":"{\"version\":3,\"file\":\"ChatForm.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/components/ChatForm.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAiB,MAAM,EAAE,SAAS,EAAE,QAAQ,EAAE,MAAM,OAAO,CAAC;AAE1E,OAAO,KAAK,MAAM,OAAO,CAAC;AAI1B,OAAO,EAAE,YAAY,EAAE,MAAM,mBAAmB,CAAC;AAEjD,OAAO,MAAM,MAAM,cAAc,CAAC;AAelC,MAAM,QAAQ,GAAG,CAAC,KAAoB,EAAE,EAAE;IACxC,MAAM,WAAW,GAAG,MAAM,CAAC,KAAK,CAAC,CAAC;IAClC,MAAM,CAAC,OAAO,EAAE,UAAU,CAAC,GAAG,QAAQ,CAAe,EAAE,CAAC,CAAC;IACzD,MAAM,CAAC,QAAQ,EAAE,WAAW,CAAC,GAAG,QAAQ,CAAe,EAAE,CAAC,CAAC;IAE3D,SAAS,CAAC,GAAG,EAAE;QACb,IAAI,CAAC,WAAW,CAAC,OAAO,EAAE;YACxB,WAAW,CAAC,OAAO,GAAG,IAAI,CAAC;YAE3B,WAAW,CACT,KAAK,CAAC,EAAE;gBACN,MAAM,UAAU,GAAG,KAAK,CAAC,MAAM,CAC7B,IAAI,CAAC,EAAE,CAAC,IAAI,CAAC,QAAQ,KAAK,KAAK,CAAC,QAAQ,CACzC,CAAC;gBACF,MAAM,OAAO,GAAG,UAAU,CAAC,GAAG,CAAC,IAAI,CAAC,EAAE;oBACpC,OAAO;wBACL,KAAK,EAAE,IAAI,CAAC,SAAS,CAAC,I